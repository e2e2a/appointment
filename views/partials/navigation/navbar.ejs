    <!-- Navbar Start -->
   
    <%
    function activeClass(link) {
        return currentUrl === link ? 'active' : '';
    }
    %>
    <%
    function profileClass(link) {
        return currentUrl === link ? 'bg-secondary' : '';
    }
    %>

    <nav class="navbar navbar-expand-lg bg-white navbar-light shadow sticky-top p-0">
        <a href="index" class="navbar-brand d-flex align-items-center px-4 px-lg-5">
            <h2 class="m-0 fst-italic"><img src="public/img/logo/logo.png" class="img-fluid" style="width: 100px; height: 45px;" alt="" ><span class="text-primary fs-1 font-italic">S</span>wift<span class="text-primary fs-1 font-italic">F</span>ix <span class="text-primary fs-1 font-italic">H</span>ub</h2>
        </a>
        <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto p-4 p-lg-0">
                <a href="/" class="nav-item nav-link <%= activeClass('/') %>">Home</a>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle <%= activeClass('/about') %> <%= activeClass('/team') %>" data-bs-toggle="dropdown">About</a>
                    <div class="dropdown-menu fade-up m-0">
                        <a href="about" class="dropdown-item">About Us</a>
                        <a href="team" class="dropdown-item">Team</a>
                        <a href="404" class="dropdown-item">404 Page</a>
                    </div>
                </div>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle <%= activeClass('/service') %> <%= activeClass('/booking') %>" id="serviceDropdown"  data-bs-toggle="dropdown">Service</a>
                    <div class="dropdown-menu fade-up m-0">
                        <a href="service" class="dropdown-item">Services</a>
                        <a href="booking" class="dropdown-item">Booking</a>
                    </div>
                </div>
                <a href="contact" class="nav-item nav-link <%= activeClass('/contact') %>">Contact</a>
                <% if (!session.userId) { %>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle  <%= activeClass('/login') %>  <%= activeClass('/register') %>" data-bs-toggle="dropdown">Login</a>
                    <div class="dropdown-menu fade-up m-0">
                        <a href="login" class="dropdown-item">Login</a>
                        <a href="register" class="dropdown-item">Register</a>
                    </div>
                </div>
                <% } else { %>
                    <%
                    const imagePath = `${user.imageURL}`;
                    %>
                <div class="nav-item me-4">
                    <% if (!user.imgUrl) { %>
                        <img src="public/img/profile/empty-profile.png" data-bs-toggle="dropdown" class="dropdown-toggle rounded-circle img-fluid img-thumbnail mt-2 mb-2  <%= profileClass('/profile') %>" style="width: 55px; height: 55px;" alt="" >
                      <% } else { %>
                        <img src="<%= user.imageURL %>" data-bs-toggle="dropdown" class="dropdown-toggle rounded-circle img-fluid img-thumbnail mt-2 mb-2  <%= profileClass('/profile') %>" style="width: 55px; height: 55px;" alt="" >
                      <% } %>
                    <div class="dropdown-menu dropdown-menu-end fade-up m-0">
                        <a href="profile" class="dropdown-item">Edit</a>
                        <form action="logout" method="post">
                            <button type="submit" class="dropdown-item">Logout</button>
                        </form>
                    </div>
                </div>
                <% } %>
            </div>
           
        </div>
    </nav>
    
    
    <!-- Navbar End -->

    
    

    
    
    
    
    
    
    
    
    
    
    
    
    
    